<Page x:Class="PhotostudioGUI.Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:PhotostudioGUI"
      mc:Ignorable="d"
      Title="Login" Height="450" Width="800">
    <Grid Name="MainGrid">
        <Label Content="Login" HorizontalAlignment="Left" Margin="332,90,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="loginBox" HorizontalAlignment="Left" Height="22" Margin="332,120,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="135"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="332,150,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Height="22" Margin="332,180,0,0" VerticalAlignment="Top" Width="135" />
        <Button Content="Login" HorizontalAlignment="Left" Height="26" Margin="332,220,0,0" VerticalAlignment="Top" Width="135" Click="Button_Click"/>
    </Grid>
</Page>
