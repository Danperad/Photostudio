<Page x:Class="PhotostudioGUI.Pages.CurrentEmployeePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
      xmlns:mD="http://materialdesigninxaml.net/winfx/xaml/themes"
      xmlns:local="clr-namespace:PhotostudioGUI.Pages"
      mc:Ignorable="d"
      Title="CurrentEmployeePage">
    <Grid>
        <Button Click="ButtonBase_OnClick" Height="35" Width="35" fw:PointerTracker.Enabled="True"
                Style="{StaticResource ButtonRevealStyle}" HorizontalAlignment="Left" VerticalAlignment="Top">
            <mD:PackIcon Kind="ArrowLeft"></mD:PackIcon>
        </Button>
    </Grid>
</Page>